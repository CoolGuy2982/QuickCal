{
    "name": "QuickCal",
    "description": "Chrome extension to easily add events to your calendar.",
    "version": "0.1",
    "manifest_version": 3,
    "action": {
      "default_popup": "quickcal/popup.html",
      "default_icon": "images/quickcal.png"
    },
    "permissions": [
        "contextMenus",
        "activeTab"
    ],
    "background": {
        "service_worker": "quickcal.js"
    },
    "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
    }
  }